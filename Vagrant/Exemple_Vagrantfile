Vagrant.configure("2") do |config|

  #Choix de la box a utiliser
  config.vm.box = "BOX A DEFINIR"
  
  #Parametrage du rÃ©seau en bridge depuis la carte "enp3s0"
  config.vm.network :public_network, :bridge => "enp3s0: Ethernet", ip: "IP A DEFINIR", :netmask => "MASQUE A DEFINIR"
  
  #Nom de la machine dans Vagrant
  config.vm.define "NOM DE LA BOX A DEFINIR"
  
end
